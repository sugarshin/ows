{
  "name": "ows",
  "version": "0.0.2",
  "description": "Simple observable like WeakSet",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/ows.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "coffeelint -f coffeelint.json src/",
    "test": "mocha --require espower-coffee/guess test/**/*.coffee",
    "compile": "coffee -c -o lib/ src/",
    "standalone": "browserify src/ows.coffee -t coffeeify -s OWS | derequire > build/ows.js",
    "minify": "uglifyjs build/ows.js -m -c warnings=false --comments '/\\/*!/' -o build/ows.min.js",
    "build": "npm run lint && npm run compile && npm run test && npm run standalone && npm run minify"
  },
  "engines": {
    "node": "0.12.x"
  },
  "author": "sugarshin",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^1.1.1",
    "isarray": "0.0.1",
    "lodash.includes": "^3.1.3",
    "lodash.remove": "^3.1.0"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "coffeelint": "^1.10.1",
    "derequire": "^2.0.0",
    "espower-coffee": "^0.10.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.23"
  }
}
